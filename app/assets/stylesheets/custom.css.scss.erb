/** navbar css **/
#navbar ul { 
	margin: 0; 
	padding: 5px; 
	padding-top: 50px;
	list-style-type: none; 
	text-align: center; 
	background-color: #fff; 
} 
 
#navbar ul li {  
	display: inline; 
	font-family: proximaNovaSemibold, proximaNova, Helvetica, Arial, sans-serif;
	text-transform: uppercase;
	font-size: 11px;
	letter-spacing: 0.4px;
} 
 
#navbar ul li a { 
	text-decoration: none; 
	padding: .2em 3.3em; 
	color: #7B7B7B; 
} 
 
#navbar ul li a:hover { 
	color: #000;
} 
